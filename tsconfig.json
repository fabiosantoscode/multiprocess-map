{
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "allowJs": false,
    "alwaysStrict": true,
    "declaration": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5",
    "lib": [
      "es2015"
    ]
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
